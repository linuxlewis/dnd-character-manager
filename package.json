{
	"name": "agent-first-template",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "concurrently \"tsx --watch src/server.ts\" \"vite --config src/app/vite.config.ts\"",
		"dev:server": "tsx --watch src/server.ts",
		"dev:web": "vite --config src/app/vite.config.ts",
		"build": "tsc --noEmit && vite build --config src/app/vite.config.ts",
		"test": "vitest run",
		"test:watch": "vitest",
		"lint": "biome check . && tsx lints/check-deps.ts",
		"lint:fix": "biome check --fix .",
		"format": "biome format --write .",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"check:docs": "tsx scripts/check-doc-freshness.ts",
		"worktree:boot": "bash scripts/worktree-boot.sh"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.0",
		"@types/node": "^25.3.0",
		"@types/react": "^19.2.14",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.4",
		"concurrently": "^9.0.0",
		"drizzle-kit": "^0.31.9",
		"drizzle-orm": "^0.45.1",
		"tsx": "^4.19.0",
		"typescript": "^5.6.0",
		"vite": "^7.3.1",
		"vitest": "^2.1.0"
	},
	"engines": {
		"node": ">=22"
	},
	"packageManager": "pnpm@9.15.0",
	"dependencies": {
		"fastify": "^5.7.4",
		"pino": "^10.3.1",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"zod": "^4.3.6"
	}
}
